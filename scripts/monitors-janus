#!/bin/sh

xrandr --dpi 163 \
    --output HDMI-A-0 --rotate left
    # --output DisplayPort-2 --mode 3840x2160 --pos 0x0 --rotate left \
    # --output HDMI-A-0 --scale 2x2 --pos 2160x0 --panning 3840x2160+2160+0 --rotate normal \
    # --output DisplayPort-0 --off

xrandr --output HDMI-A-0 --primary
